    "full-dev": "concurrently \"npm run dev\" \"npm run backend\""
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@types/qrcode": "^1.5.5",
    "@xenova/transformers": "^2.17.2",
    "firebase": "^11.3.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.6",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
