{
  "name": "twise-feedback",
  "version": "1.0.0",
  "private": true,
  "description": "Universal Feedback Platform for events with real-time sentiment analysis",
  "keywords": [
    "feedback",
    "analytics",
    "sentiment-analysis",
    "events",
    "real-time"
  ],
  "author": "sambett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sambett/twise-feedback.git"
  },
  "homepage": "https://sambett.github.io/twise-feedback",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next node_modules/.cache",
    "reset": "npm run clean && npm install",
    "type-check": "tsc --noEmit",
